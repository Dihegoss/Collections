package javaapplication3;

import java.util.TreeSet;

/**
 *
 * @author Diego
 */
public class JavaApplication3 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        TreeSet <Integer> numero = new TreeSet();
        
       numero.add(3);
       numero.add(88);
       numero.add(20);
       numero.add(44);
       numero.add(3);
       
       
       //Navegando pela arvore
       for(Integer num : numero){
           System.out.println(num);
           System.out.println(" ");      
       }
       System.out.println("Exibindo o primeiro da arvore e removodo-o");
        System.out.println(numero.pollFirst());
        System.out.println(" ");
        
        numero.add(23);
        System.out.println(numero);
        System.out.println(" ");
        
        System.out.println("Tamanho do set");
        System.out.println(numero.size());
        
        System.out.println(" ");
        System.out.println("Verificando se o Set est√° vazio:");
        System.out.println(numero.isEmpty());
    }
    
}
